{
  "compilerOptions": {
    // "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/declaration.d.ts"]
}
